  "dependencies": {
    "lodash": "^4.17.15",
    "luxon": "^1.28.1",
    "svg4everybody": "^2.1.9",
    "vue": "^2.6.10",
    "vue-datetime": "^1.0.0-beta.11",
    "vue-outside-events": "^1.1.3",
